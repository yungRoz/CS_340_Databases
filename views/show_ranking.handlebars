<table id="rankingTable" align="center" class="pure-table pure-table-horizontal">
  <tr class="header">
    <th>Name</th>
    <th>Rating</th>
  </tr>
  {{#if results}}
    {{#each results}}
      {{#if this.hiName}}
        <tr class="row">
          <td>{{this.hiName}}</td>
          <td>{{this.hiRating}}</td>
        </tr>
      {{/if}}
    {{/each}}
  {{/if}}
  {{#if results}}
    {{#each results}}
      {{#if this.name}}
        <tr class="row">
          <td>{{this.name}}</td>
          <td>{{this.rating}}</td>
        </tr>
      {{/if}}
    {{/each}}
  {{/if}}
  {{#if results}}
    {{#each results}}
      {{#if this.loName}}
        <tr class="row">
          <td>{{this.loName}}</td>
          <td>{{this.loRating}}</td>
        </tr>
      {{/if}}
    {{/each}}
  {{/if}}
</table>
